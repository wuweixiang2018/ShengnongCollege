<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
	    <shape>
	        <solid android:color="@drawable/common_btn_blue_pressed" />
	    	<corners android:radius="4dp"/>
		</shape>
	</item>
    <item android:state_enabled="false" >
	    <shape>    
	        <solid android:color="@drawable/common_btn_blue_disable" />
	    	<corners android:radius="4dp"/>
		</shape>
	</item>
    <item>
	    <shape>    
	        <solid android:color="@drawable/common_btn_blue_normal" />
	    	<corners android:radius="4dp"/>
		</shape>
	</item>
</selector>
